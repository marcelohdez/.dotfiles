profile laptop {
  output eDP-1 enable
  exec ~/.config/sway/scripts/lid_toggle.sh disc
}

profile desktop {
  output 'Dell Inc. AW2724DM 7TLZHV3' position 0,0
  output 'LG Electronics MP59G 0x00039DF3' position 2560,250
}

profile laptopLab {
  output 'Dell Inc. DELL P2418HT 488CG71UCMQL' position 1050,0
  output eDP-1 position 0,1080
  exec ~/.config/sway/scripts/lab_monitors.sh
}
